# MyLinear 环境变量配置
# 复制此文件为 .env 并根据需要修改

# 数据库配置
DATABASE_URL=postgres://mylinear:mylinear@localhost:5432/mylinear?sslmode=disable

# Redis 配置
REDIS_URL=redis://localhost:6379/0

# 服务端口
PORT=8080

# MinIO 配置 (S3 兼容对象存储)
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
MINIO_BUCKET=mylinear
